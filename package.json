{
  "name": "topcoder-learning-paths-api",
  "version": "1.0.0",
  "description": "Topcoder Learning Paths V5 API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:debug": "DEBUG=express:* node app.js",
    "dev": "nodemon app.js",
    "generate-courses": "node src/utils/course-generator/generate-courses.js",
    "lint": "eslint src --ext js,jsx",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "rebuild-tables": "npm run drop-tables && npm run create-tables && npm run seed-tables",
    "services:up": "docker-compose -f ./local/docker-compose.yml up -d",
    "services:down": "docker-compose -f ./local/docker-compose.yml down",
    "services:logs": "docker-compose -f ./local/docker-compose.yml logs",
    "cert-gen:deploy-stack": "cd src/utils/certificate-sharing/generate-certificate-image && sh generate-certificate-image-deploy.sh",
    "cert-gen:regenerate": "node src/utils/certificate-sharing/regenerate-certificate-images/RegenerateCertificateImagesService.js",
    "cert-gen:validate": "d src/utils/certificate-sharing/generate-certificate-image && aws cloudformation validate-template --template-body file://certificate-image-generator-stack.yml"
  },
  "author": "Topcoder.com",
  "license": "UNLICENSED",
  "repository": "https://github.com/topcoder-platform/learning-paths-api",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.17.0",
    "mocha": "^10.0.0",
    "mocha-prepare": "^0.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.5.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.315.0",
    "@aws-sdk/client-sqs": "^3.315.0",
    "@paralleldrive/cuid2": "^2.0.1",
    "@prisma/client": "^4.5.0",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dd-trace": "^2.12.1",
    "dotenv": "^16.0.1",
    "dynamoose": "^2.8.5",
    "express": "^4.18.1",
    "express-interceptor": "^1.2.0",
    "get-parameter-names": "^0.3.0",
    "http-aws-es": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mongodb": "^4.8.1",
    "node-cache": "^5.1.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.27.0",
    "sequelize-auto": "^0.8.8",
    "stripe": "^11.3.0",
    "swagger-ui-express": "^4.4.0",
    "tc-core-library-js": "topcoder-platform/tc-core-library-js.git#master",
    "topcoder-bus-api-wrapper": "topcoder-platform/tc-bus-api-wrapper.git",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "xss": "^1.0.11",
    "yamljs": "^0.3.0"
  },
  "standard": {
    "ignore": [
      "mock-api"
    ],
    "env": [
      "mocha"
    ]
  },
  "engines": {
    "npm": ">= 8.5.5",
    "yarn": "please-use-npm",
    "node": "22.x"
  },
  "volta": {
    "node": "22.21.1"
  }
}