{
    "parserOptions": {
        "es6": true,
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        },
        "requireConfigFile": false
    },
    "rules": {
        "no-unused-vars": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}